{
  "name": "lerna-monorepo-starter",
  "description": "Lerna monorepo starter",
  "version": "1.0.0",
  "author": "Andrico Karoulla <karoulla.andrico@gmail.com>",
  "dependencies": {
    "gatsby": "^2.17.11",
    "gatsby-link": "^2.2.24",
    "gatsby-plugin-react-helmet": "^3.1.14",
    "gatsby-plugin-react-next": "^1.0.11",
    "gatsby-source-filesystem": "^2.1.36",
    "gatsby-transformer-json": "^2.2.18",
    "npm-run-all": "^4.1.5",
    "react": "^16.11.0",
    "react-helmet": "^5.2.1",
    "react-router-dom": "^5.1.2",
    "serve": "^10.0.0",
    "styled-components": "^3.3.3"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "test": "jest test",
    "run:news": "cd packages/news && yarn develop",
    "run:blog": "cd packages/blog && yarn develop",
    "run:shop": "cd packages/shop && yarn develop",
    "run:all": "npm-run-all --parallel run:news run:shop run:blog"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "jest": "^24.9.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-default"
  },
  "private": true,
  "workspaces": [
    "packages/*"
  ]
}
